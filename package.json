{
  "name": "@w3bstream/wasm-sdk",
  "version": "0.1.7",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/machinefi/w3bstream-wasm-ts-sdk.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "node tests",
    "asbuild:log": "asc examples/log/index.ts --use abort=assembly/memory/abort --target release -o ./build/log.wasm",
    "asbuild:json": "asc examples/json/index.ts --use abort=assembly/memory/abort --target release -o ./build/json.wasm",
    "asbuild:word_count": "asc examples/word_count/index.ts --use abort=assembly/memory/abort --target release -o ./build/word_count.wasm",
    "asbuild:token_distribute": "asc examples/token_distribute/index.ts --use abort=assembly/memory/abort --target release -o ./build/token_distribute.wasm",
    "asbuild:erc20_mint": "asc examples/erc20_mint/index.ts --use abort=assembly/memory/abort --target release -o ./build/erc20_mint.wasm",
    "asbuild:erc721_mint": "asc examples/erc721_mint/index.ts --use abort=assembly/memory/abort --target release -o ./build/erc721_mint.wasm",
    "asbuild:env": "asc examples/env/index.ts --use abort=assembly/memory/abort --target release -o ./build/env.wasm",
    "asbuild:sql": "asc examples/sql/index.ts --use abort=assembly/memory/abort --target release -o ./build/sql.wasm",
    "start": "npx serve ."
  },
  "author": "@w3bstream",
  "license": "ISC",
  "dependencies": {
    "assemblyscript": "^0.26.7",
    "assemblyscript-json": "^1.1.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  },
  "devDependencies": {
    "assemblyscript": "^0.26.7",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.2",
    "semantic-release": "^20.1.0"
  }
}
